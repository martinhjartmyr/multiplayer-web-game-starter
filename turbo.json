{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["GAME_DEBUG"],
  "tasks": {
    "clean": {
      "dependsOn": []
    },
    "build": {
      "outputs": ["dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
